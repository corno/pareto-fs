{
  "name": "pareto-fs",
  "version": "0.3.3",
  "description": "fs functionality for pareto projects",
  "main": "dist/index.js",
  "scripts": {
    "build": "rd /s /q .\\dist && tsc --p ./src/lib",
    "buildGenerator": "rd /s /q .\\intermediates && tsc --p ./src/generateFsFunctionWrappers",
    "lint": "tslint --project tsconfig.json ./src/*",
    "pubMin": "git diff --exit-code && ncu -u --packageFile package.json && npm install && rd /s /q .\\dist && npm run build && npm run lint && npm run test && npm version minor && git push && npm publish",
    "pubPatch": "git diff --exit-code && ncu -u --packageFile package.json && npm install && rd /s /q .\\dist && npm run build && npm run lint && npm run test && npm version patch && git push && npm publish",
    "test": "npm run build && node ./dist/test",
    "u2l": "ncu -u --packageFile package.json && nsi",
    "generate": "node ./intermediates/generateFsFunctionWrappers"
  },
  "keywords": [],
  "author": "corno",
  "license": "ISC",
  "types": "dist/index.d.ts",
  "prepublish": "tsc",
  "repository": {
    "type": "git",
    "url": "https://github.com/corno/pareto-fs.git"
  },
  "files": [
    "dist/*"
  ],
  "dependencies": {
    "@types/node": "^12.12.7",
    "pareto": "^0.26.0",
    "pareto-20": "0.0.2"
  },
  "devDependencies": {
    "lingua-franca-building": "^0.2.0",
    "steroid-template-utils": "^0.6.0"
  }
}
